{
  "manifest_version": 3,
  "name": "Word Meaning Finder",
  "version": "1.0",
  "description": "Shows meaning of selected English word using DictionaryAPI.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://api.dictionaryapi.dev/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
